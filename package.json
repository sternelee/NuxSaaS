{
  "name": "nuxsaas",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "test": "vitest run --silent=false",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "auth:schema": "npx @better-auth/cli generate --config server/utils/auth.ts --output server/database/schema/auth.ts -y",
    "db:generate": "drizzle-kit generate --config ./server/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./server/database/drizzle.config.ts",
    "build": "nuxt build",
    "serve": "node .output/server/index.mjs",
    "preview:cf": "wrangler dev --x-remote-bindings",
    "deploy": "wrangler deploy",
    "prepare": "husky"
  },
  "dependencies": {
    "@better-auth/stripe": "^1.3.11",
    "@nuxt/ui": "^3.3.4",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@nuxtjs/seo": "^3.2.2",
    "@polar-sh/better-auth": "^1.1.4",
    "@polar-sh/sdk": "^0.34.17",
    "@vueuse/core": "13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "aws4fetch": "^1.0.20",
    "better-auth": "^1.3.11",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "drizzle-orm": "0.44.5",
    "echarts": "^5.6.0",
    "ioredis": "^5.7.0",
    "nuxt": "^4.1.2",
    "nuxt-echarts": "^0.4.0",
    "ofetch": "^1.4.1",
    "pg": "^8.16.3",
    "resend": "^6.1.0",
    "sortablejs": "^1.15.6",
    "stripe": "^18.5.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.21",
    "vue-component-type-helpers": "3.0.7",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@better-auth/cli": "^1.3.11",
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/simple-icons": "^1.2.52",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@types/pg": "^8.15.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "playwright-core": "^1.55.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.37.1"
  }
}
